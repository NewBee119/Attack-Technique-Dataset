<script>utag_data.content_cat_1 = "Resources";utag_data.content_cat_2 = "Threat Intelligence Reports";utag_data.content_cat_3 = "Hacking the Street? FIN4 Likely Playing the Market";utag_data.content_cat_3 = "Hacking the Street? FIN4 Likely Playing the Market";</script>
<script type="text/javascript">
    var pathname=window.location.pathname;

    var urldepth=window.location.pathname;
    // Normal page with content_Category_1
    if(urldepth=="/")
    {}
    else
    if(urldepth.match(/\/.+?/g).length==1)
    {
        utag_data.tealium_event="content_category_1";
        utag_data.page_type="content_category_1";
    }
    // Normal page with content_Category_2
    else 
    if(urldepth.match(/\/.+?/g).length>=2)
    {
        utag_data.tealium_event="content_category_2";
        utag_data.page_type="content_category_2";
    }
    //Check if page is datasheet
    if(pathname.includes("datasheet"))
    {
            utag_data.tealium_event="data_sheets";
        	utag_data.page_type="data_sheets";
    }
    //check if page is press release
    /*if(pathname.endsWith("press-releases.html"))
    {
        utag_data.tealium_event="listing";
    }
    if((pathname.startsWith("/solutions"))&&(pathname.match(/\/.+?/g).length==3))
    {
        utag_data.tealium_event="landing";
        }*/
    //Check if page is a blog
    if(pathname.startsWith("/blog"))
    {
        utag_data.tealium_event="blog";
        utag_data.page_type="blog";
    }
    if(window.location.pathname.endsWith("contacts.html") || window.location.pathname.endsWith("contact-us.html"))
    {
        utag_data.tealium_event="contact";
        utag_data.page_type="contact";
    }
    if(pathname.endsWith("customers.html"))
    {
        utag_data.tealium_event="filter";
        utag_data.page_type="filter";
    }
    //Check if page is homepage
    if(window.location.pathname==="/")
    {
        // utag_data.page_type='';
        utag_data.tealium_event="home";
        utag_data.page_type="home";
        delete utag_data.content_cat_1;

    }
    else
        //Check if page is job description page
    if(window.location.pathname.endsWith("jobdescription.html"))
        {
            var jobdescription=getParameterByName("jobtitle");
            var jobloc=getParameterByName("loc");
            utag_data.job_description=jobdescription+":"+jobloc;
            utag_data["tealium_event"]="job description";
            utag_data.page_type="job description";

        }
        //Function to get query parameter
        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
</script>
